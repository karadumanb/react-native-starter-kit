{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "paths": {
      "screens/*": ["screens/*"],
      "navigation/*": ["navigation/*"],
      "components/*": ["components/*"],
      "lib/*": ["lib/*"],
      "hooks/*": ["lib/hooks/*"],
      "assets/*": ["assets/*"],
      "constants/*": ["lib/constants/*"],
      "modules/*": ["modules/*"],
      "contexts/*": ["lib/contexts/*"],
      "services/*": ["services/*"],
      "concepts/*": ["concepts/*"],
    }
  }
}
